    "environment_patterns": [
      {"column_pattern": ".*environment.*", "confidence": 0.95},
      {"column_pattern": ".*env.*", "confidence": 0.9},
      {"column_pattern": ".*stage.*", "confidence": 0.8},
      {"column_pattern": ".*tier.*", "confidence": 0.7}
    ],
    "application_patterns": [
      {"column_pattern": ".*application.*", "confidence": 0.95},
      {"column_pattern": ".*app.*name.*", "confidence": 0.9},
      {"column_pattern": ".*service.*name.*", "confidence": 0.9},
      {"column_pattern": ".*app.*", "confidence": 0.7},
      {"column_pattern": ".*service.*", "confidence": 0.7},
      {"column_pattern": ".*component.*", "confidence": 0.6}
    ],
    "owner_patterns": [
