  "output_database": "bigquery_cmdb.db",
  "checkpoint": {
    "enabled": true,
    "checkpoint_dir": "checkpoints",
    "checkpoint_interval": 1000
  },
  "export": {
    "csv": true,
    "json": true,
    "parquet": false,
    "output_dir": "output"
  },
  "logging": {
    "level": "INFO",
    "file": "logs/bigquery_cmdb.log",
    "console": true,
    "format": "detailed"
  },
  "performance": {
    "max_workers": 4,
    "batch_size": 100,
    "enable_caching": true,
    "cache_ttl_seconds": 3600
  }
}