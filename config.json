{
  "bigquery": {
    "projects": [
      "REPLACE-WITH-YOUR-PROJECT-ID-1",
      "REPLACE-WITH-YOUR-PROJECT-ID-2"
    ],
    "credentials_path": "gcp_credentials.json",
    "datasets_filter": [],
    "tables_filter": [],
    "sample_percent": 10,
    "max_rows_per_table": 100000,
    "timeout_seconds": 60
  },
  "discovery": {
    "hostname_patterns": [
      {"column_pattern": ".*hostname.*", "confidence": 0.95},
      {"column_pattern": ".*host_name.*", "confidence": 0.95},
      {"column_pattern": ".*server.*name.*", "confidence": 0.9},
      {"column_pattern": ".*instance.*name.*", "confidence": 0.9},
      {"column_pattern": ".*machine.*name.*", "confidence": 0.85},
      {"column_pattern": ".*device.*name.*", "confidence": 0.85},
      {"column_pattern": ".*node.*name.*", "confidence": 0.85},
      {"column_pattern": ".*computer.*name.*", "confidence": 0.85},
      {"column_pattern": ".*fqdn.*", "confidence": 0.95},
      {"column_pattern": ".*host.*", "confidence": 0.7},
      {"column_pattern": ".*server.*", "confidence": 0.6},
      {"column_pattern": ".*instance.*", "confidence": 0.6},
      {"column_pattern": ".*node.*", "confidence": 0.6}
    ],
    "ip_patterns": [
      {"column_pattern": ".*ip.*address.*", "confidence": 0.95},
      {"column_pattern": ".*ip_addr.*", "confidence": 0.95},
      {"column_pattern": ".*ipv4.*", "confidence": 0.95},
      {"column_pattern": ".*ipv6.*", "confidence": 0.95},
      {"column_pattern": ".*private.*ip.*", "confidence": 0.9},
