    "location_patterns": [
      {"column_pattern": ".*region.*", "confidence": 0.95},
      {"column_pattern": ".*zone.*", "confidence": 0.9},
      {"column_pattern": ".*datacenter.*", "confidence": 0.95},
      {"column_pattern": ".*dc.*", "confidence": 0.7},
      {"column_pattern": ".*location.*", "confidence": 0.8},
      {"column_pattern": ".*site.*", "confidence": 0.7}
    ],
    "cluster_patterns": [
      {"column_pattern": ".*cluster.*", "confidence": 0.95},
      {"column_pattern": ".*pool.*", "confidence": 0.7},
      {"column_pattern": ".*group.*", "confidence": 0.6}
    ],
    "project_patterns": [
